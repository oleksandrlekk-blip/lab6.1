namespace LabPolymorphism;

public class Airplane : Vehicle, IRefuelable
{
    public Airplane(string brand, int speed) : base(brand, speed) { }

    public override void Move()
    {
        Console.WriteLine($"Літак {brand} летить у небі зі швидкістю {speed} км/год.");
    }

    public void Refill()
    {
        Console.WriteLine($"Літак {brand} заправляється авіаційним пальним.");
    }
}
